{   "load_latest_model": false,
    "unfreezing": false,
    "approach": "C3D",
    "optimizer": "Adagrad",
    "activation": "relu",
    "padding": "same",
    "epochs": 100,
    "filters": 32,
    "dropout": 0.5,
    "learning_rate": 0.001,
    "kernel_size": [3, 3, 3],
    "pooling_strides": [2, 2, 2],
    "callbacks": {
        "model_checkpoint": {
            "disabled": false
        },
        "csv_logger": {
            "disabled": false, 
            "append": true
        },
        "early_stopping": {
            "disabled": false,
            "monitor": "val_loss",
            "patience": 10
        },
        "learning_rate_scheduler": {
            "disabled": false,
            "starts_at": 9,
            "every": 5
        },
        "plot_losses_keras": {
            "disabled": true
        }
    }
}